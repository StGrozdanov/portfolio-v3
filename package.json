{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "type-check": "tsc --pretty --noEmit",
    "type-check:tests": "tsc --pretty --noEmit --project test/tsconfig.json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint"
    ],
    "*.{js}": "eslint --cache --fix",
    "*.{ts,js,json,md}": "prettier --write"
  },
  "dependencies": {
    "lint-staged": "^15.1.0",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "husky": "^8.0.0",
    "sass": "^1.69.5",
    "typescript": "^5"
  }
}
