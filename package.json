{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --ext ts --ext tsx --ext js",
    "type-check": "tsc --pretty --noEmit",
    "type-check:tests": "tsc --pretty --noEmit --project test/tsconfig.json",
    "prepare": "husky install",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:int": "playwright test tests/integration --pass-with-no-tests",
    "test:e2e": "playwright test tests/e2e --pass-with-no-tests"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint"
    ],
    "*.{js}": "eslint --cache --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.0",
    "@fortawesome/free-solid-svg-icons": "^6.5.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@vercel/analytics": "^1.1.1",
    "animate.css": "^4.1.1",
    "emailjs-com": "^3.2.0",
    "lint-staged": "^15.1.0",
    "moment": "^2.29.4",
    "next": "14.0.3",
    "pg": "^8.11.3",
    "rc-progress": "^3.5.1",
    "react": "^18",
    "react-animation-on-scroll": "^5.1.0",
    "react-dom": "^18",
    "react-responsive": "^9.0.2",
    "react-responsive-carousel": "^3.2.23",
    "react-scroll": "^1.9.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "6.1.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "^29.5.10",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-scroll": "^1.8.10",
    "@types/testing-library__react": "^10.2.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-func": "^0.1.18",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "29.6.4",
    "sass": "^1.69.5",
    "typescript": "^5.2.2"
  }
}
